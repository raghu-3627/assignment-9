#include <iostream>
using namespace std;

struct Item {
    int id;
    float cost;
};

int main(){
    Item items [6] = {
        {501, 46.9},
        {502, 69},
        {503, 78.5},
        {504, 30.0},
};

items[4].id = 505;
items[4].cost = 56.9;

items[5].id = 506;
items[5].cost = 49.9;

cout << "items with cost greater than 50.00:-\n";


for(int i = 0; i < 6; i++){
    if(items[i].cost > 50.00){
        cout <<"Item ID: "<< items[i].id << ", cost: "<< items[i].cost<< endl;
        }
}
return 0 ;
}
